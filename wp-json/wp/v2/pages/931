e\u043d\u0442\u0435\u0439\u043d\u0435\u0440 \u0456\u043a\u043e\u043d\u043e\u043a\n    iconContainer.appendChild(button);\n\n    \/\/ \u041e\u0431\u0440\u043e\u0431\u043d\u0438\u043a \u043f\u043e\u0434\u0456\u0439 \u0434\u043b\u044f \u0456\u043a\u043e\u043d\u043a\u0438 \u0437\u0430\u0432\u0430\u043d\u0442\u0430\u0436\u0435\u043d\u043d\u044f\n    iconSpan.addEventListener('click', function() {\n        fileInput.click();  \/\/ \u0421\u0438\u043c\u0443\u043b\u044f\u0446\u0456\u044f \u043a\u043b\u0456\u043a\u0443 \u043f\u043e input[type='file']\n    });\n\n    fileInput.addEventListener('change', function() {\n        if (fileInput.files.length > 0) {\n            var fileName = fileInput.files[0].name;\n            button.value = truncateFileName(fileName, 20); \/\/ \u0412\u0438\u043a\u043e\u0440\u0438\u0441\u0442\u0430\u043d\u043d\u044f \u0444\u0443\u043d\u043a\u0446\u0456\u0457 truncateFileName\n            closeIconSpan.firstChild.style.display = 'block'; \/\/ \u041f\u043e\u043a\u0430\u0437 \u0456\u043a\u043e\u043d\u043a\u0438 \u0437\u0430\u043a\u0440\u0438\u0442\u0442\u044f\n            iconSpan.firstChild.style.display = 'none';\n        } else {\n            resetButton();\n        }\n    });\n\n    closeIconSpan.addEventListener('click', function() {\n        fileInput.value = ''; \/\/ \u0421\u043a\u0438\u0434\u0430\u043d\u043d\u044f \u0432\u0438\u0431\u0440\u0430\u043d\u043e\u0433\u043e \u0444\u0430\u0439\u043b\u0443\n        resetButton();\n    });\n\n    function resetButton() {\n        button.value = 'Attach CV';\n        closeIconSpan.firstChild.style.display = 'none';\n        iconSpan.firstChild.style.display = 'block';\n    }\n\n    \/\/ \u0424\u0443\u043d\u043a\u0446\u0456\u044f \u0434\u043b\u044f \u043e\u0431\u0440\u0456\u0437\u0430\u043d\u043d\u044f \u043d\u0430\u0437\u0432\u0438 \u0444\u0430\u0439\u043b\u0443\n    function truncateFileName(name, maxLength) {\n        return name.length > maxLength ? name.substring(0, maxLength) + '...' : name;\n    }\n});\n<\/script><div class=\"wpcf7-response-output\" aria-hidden=\"true\"><\/div>\n<\/form>\n<\/div>\n<\/div>\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-eaa8d08 elementor-widget elementor-widget-html\" data-id=\"eaa8d08\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t<script>\ndocument.addEventListener('DOMContentLoaded', function() {\n    \/\/ \u041f\u0440\u043e\u0441\u043b\u0443\u0445\u043e\u0432\u0443\u0432\u0430\u043d\u043d\u044f \u043f\u043e\u0434\u0456\u0457 \u043d\u0435\u0432\u0434\u0430\u043b\u043e\u0457 \u0432\u0456\u0434\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u043d\u044f \u0444\u043e\u0440\u043c\u0438\n    document.addEventListener('wpcf7mailfailed', function(event) {\n        if (elementorFrontend && elementorFrontend.documentsManager) {\n            var popup = elementorFrontend.documentsManager.documents[1824];\n            if (popup) {\n                popup.showModal();\n            } else {\n                console.error(\"Elementor popup with ID 1824 not found\");\n            }\n        } else {\n            console.error(\"Elementor frontend modules not available\");\n        }\n    });\n\n    \/\/ \u041f\u0440\u043e\u0441\u043b\u0443\u0445\u043e\u0432\u0443\u0432\u0430\u043d\u043d\u044f \u043f\u043e\u0434\u0456\u0457 \u0443\u0441\u043f\u0456\u0448\u043d\u043e\u0457 \u0432\u0456\u0434\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u043d\u044f \u0444\u043e\u0440\u043c\u0438\n    document.addEventListener('wpcf7mailsent', function(event) {\n        if (elementorFrontend && elementorFrontend.documentsManager) {\n            var popup = elementorFrontend.documentsManager.documents[2086];\n            if (popup) {\n                popup.showModal();\n            } else {\n                console.error(\"Elementor popup with ID 2086 not found\");\n            }\n        } else {\n            console.error(\"Elementor frontend modules n